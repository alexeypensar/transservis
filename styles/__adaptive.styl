	}







}


@media screen and (max-width: 991.98px) {
	.container {
		max-width: 768px;
	}

	body {
		font-size: 14px;
	}

	.header {
		padding-bottom: 30px;
	}

	.header__advantages {
		margin-left: 40px;
	}
	.header__nav {
		padding: 18px;
	}

	.header__link_logo {
		margin-right: 15px;
		min-width: 100px;
	}

	.header__link_phone {
		margin-left: 15px;

		&::before {
			top: 0;
			left: -9px;
		}
	}

	.header__link {
		font-size: 0.8rem;
	}

	.header__link_arrow {
		//margin-right: 4px;
		padding-right: 4px;

		&::after {
			top: -2px;
		}
	}

	.calculator__wrapper {
		padding-top: 30px;
		padding-bottom: 30px;

	}

	.calculator {
		padding: 40px;
	}

	.calculator__inputs_top {
		flex-direction: column;
		align-items: flex-start;
	}

	.calculator__inputs {
		&_cities, &_numbers {
			width: 100%;
			& .calculator__input {
				width: 50%;
			}
		}

		&_cities {
			margin-bottom: 15px;
			& input:first-child {
				border-top-right-radius: 0;
				border-bottom-right-radius: 0;
			}

			& input:last-child {
				border-top-left-radius: 0;
				border-bottom-left-radius: 0;
			}
		}
	}

	.advantages {
		padding-top: 30px;
		padding-bottom: 30px;
	}

	.advantages__item {
		width: 48%;
		min-height: 210px;
	}

	.services__wrapper {
		padding-top: 30px;
		padding-bottom: 30px;
	}

	.services__text {
		width: 50%;
	}

	.services__info {
		width: 100%;
	}

	.services__item_large {
		width: 48%;
		padding: 30px 20px;
	}

	.services_bottom {
		flex-wrap: wrap;
	}

	.services__item:not(.services__item_large) {
		width: 48%;
		height: 240px;

		&:nth-child(3), &:nth-child(4) {
			margin-top: 20px;
		}
	}

	.about-us {
		padding-top: 30px;
		padding-bottom: 30px;
	}

	.about-us__info {
		flex-direction: column-reverse;


	}

	.about-us__image {
		margin-top: -80px;
		margin-bottom: 20px;
		width: 60%;
	}

	.about-us__content {
		width: 100%;
		margin-left: 0;
		padding: 120px 20px;
	}

	.about-us__numbers {
		flex-wrap: wrap;
	}

	.about-us__number {
		width: 50%;
		display: flex;
		flex-direction: column;
		align-items: center;
	}

	.callback {
		margin-top: 30px;
		margin-bottom: 30px;
		padding: 30px 40px;
	}



	.callback__inputs {
		flex-wrap: wrap;
		& .main-button {
			margin-left: 0;
			margin-top: 20px;
		}
	}

	.our-clients {
		padding-top: 30px;
	}

	.footer {
		margin-top: 30px;
	}

	.footer__inner {
		flex-direction: column-reverse;
	}

	.footer__info {
		width: 100%;
		padding-right: 0;
		height: auto;

		&_top {
			margin-bottom: 10px;
		}

		&_bottom {
			margin-bottom: 20px;
		}


	}


	.footer__addresses {
		margin-top: 0;
		width: 100%;
		bottom: 0;
		padding: 30px 20px;
	}


	.type__text:not(:last-child) {
		margin-bottom: 15px;
	}

	.type__content {
		margin-bottom: 30px;
	}

	.type .about-us__numbers {
		padding-bottom: 30px;
	}

	.questions__item {
		width: 100%;

		&_active .questions__answer {
			margin-top: 15px;
		}
	}

	.questions__answer {
		padding-left: 30px;
		padding-right: 40px;


	}

	.contacts__item:not(:last-child) {
		margin-bottom: 40px;
	}

	.contacts__heading {
		line-height: 1.3;
	}









}

@media screen and (max-width: 767.98px) {
	.container {
		max-width: 576px;
	}

	.section__heading {
		font-size: 2rem;
		line-height: 1.3;
	}

	.header__ul {
		display: none;
	}

	.header__ul_active {
		display: flex;
		position: fixed;
		top: 0;
		right: 0;
		flex-direction: column;
		justify-content: flex-start;
		align-items: flex-start;
		padding-left: 30px;
		//padding-right: 30px;
		padding-top: 10px;
		background-color: $color-blue;
		width: auto;
		min-width: 50vw;
		height: calc(100vh);
		//margin-top: 70px;
		z-index: 99998;

		& .header__link {
			color: #FFFFFF;

			&:hover, &:focus {
				color: #FFFFFF;
				text-decoration: underline;
			}
		}
	}

	.header__li_hidden {
		display: flex;
		position: relative;
		top: 0;
		right: 0;
		z-index: 99999;
		margin-left: auto;

	}

	.mobile-menu__button_close {
		width: 20px;
		height: 20px;
		border: none;
		outline: none;
		background-color: transparent;
		z-index: 99999;
		position: relative;
		right: 20px;
		top: 20px;
		padding: 0;

		& span {
			position: relative;
			top: 0;
			left: 0;
			width: 20px;
			height: 20px;
			display: flex;


			&::before {
				content: '';
				position: absolute;
				left: 0;
				top: 6px;
				width: 20px;
				height: 2px;
				border-radius: 5px;
				background-color: #FFFFFF;
				transform: rotate(-45deg);
			}

			&::after {
				content: '';
				position: absolute;
				left: 0;
				top: 6px;
				width: 20px;
				height: 2px;
				border-radius: 5px;
				background-color: #FFFFFF;
				transform: rotate(45deg);
			}
		}
	}

	.header__link_logo {
		margin-right: auto;
		min-width: 90px;
		width: 90px;
		//flex-grow: 2;
		//width: 80%;
	}

	.header__ul {
		//width: auto;
	}

	.mobile-menu__button {
		display: flex;
	}

	.header__li_arrow {
		display: flex;
		flex-direction: column;
		align-items: flex-start;
		height: auto;



	}

	.header__link_arrow {
		&::after {
			display: none;
		}
	}

	.header__submenu {
		display: flex;
		flex-direction: column;
		justify-content: space-between;
		align-items: flex-start;
		visibility: visible;
		height: auto;
		opacity: 1;
		background-color: transparent;
		padding-top: 0;
		position: static;

		&_about::before {
			display: none;
		}

		&_types::before {
			display: none;
		}

		& li {
			width: 100%;
		}


		//& .header__link {
		//	display: flex;
		//}
	}



	//.header__li_logo {
	//	//flex-grow: 3;
	//	//margin-left: 0;
	//	//margin-right: auto;
	//	flex-grow: 1;
	//	//margin-left: 0;
	//	//margin-right: auto;
	//	justify-content: flex-start;
	//}

	.header__link_phone {
		margin-right: 15px;
		//margin-left: auto;
	}

	.header_left {
		flex-direction: column;
		padding-top: 15px;
	}

	.header__h1 {
		margin-bottom: 15px;
		line-height: 1.3;
	}

	.header__h2 {
		margin-bottom: 20px;
	}

	.header__advantages {
		margin-left: 0;
		margin-top: 20px;
	}

	.calculator__heading {
		font-size: 2rem;
		line-height: 1.3;
	}

	.calculator__inputs_bottom {
		flex-direction: column;
	}

	.calculator .main-button {
		margin-top: 20px;
	}

	.advantages__item {
		min-height: 250px;
	}

	.services_top {
		flex-direction: column;
	}

	.services__text {
		width: 100%;

	}

	.services__info {
		padding-right: 0;
	}

	.services__item_large {
		width: 100%;
		margin-top: 20px;
		height: 200px;

		& img {
			right: 20px;
			bottom: 20px;
		}
	}

	.services__item:not(.services__item_large) {
		padding: 20px 15px;
	}

	.callback__heading {
		font-size: 2rem;
		line-height: 1.3;
	}

	.our-clients .embla {
		margin-left: 20px;
		margin-right: 20px;
	}

	.our-clients .embla__slide {
		min-width: 33%;
		padding-right: 15px;
	}

	.footer__copyright {
		margin-bottom: 15px;
		margin-top: 20px;
	}

	.type__header {
		padding: 0 30px;
	}

	.type__heading {
		font-size: 2rem;
	}

	.railway__heading, .multimodal__heading {
		font-size: 1.6rem;
	}

	.documents__link {
		text-decoration: underline;
		line-height: 1.3;
		&::after {
			display: none;
		}
	}


}

@media screen and (max-width: 575.98px) {

	.header .main-button {
		width: 100%;
	}

	.header__li_logo {
		max-width: 50%;
	}

	.header__link_phone {
		white-space: nowrap;
		//margin-right: 0;
	}

	.header_right .header__embla-buttons {
		width: 80px;
		height: 40px;
		bottom: 40px;
	}

	.header_right .header__embla_prev, .header_right .header__embla_next {
		width: 40px;
		height: 40px;
	}

	.calculator {
		padding: 30px 15px;
	}

	.calculator__heading {
		font-size: 1.4rem;
	}

	.calculator__input {
		font-size: 0.7rem;
	}

	.calculator .main-button {
		width: 100%;
		margin-left: 0;
	}

	.advantages__item {
		padding: 20px 15px;
		margin-bottom: 20px;
	}

	.advantages .section__heading {
		font-size: 2rem;
	}

	.services__item_large {
		padding: 20px 15px;
	}

	.services__heading {
		margin-bottom: 10px;
		line-height: 1.2;
	}

	.services__item:hover .services__link, .services__item:focus .services__link {
		bottom: 20px;
	}

	.about-us__number span {
		font-size: 3rem;
	}

	.about-us__number p {
		text-align: center;
	}

	.callback {
		padding: 25px;
	}

	.callback .main-button {
		width: 100%;
		height: 60px;
	}

	.our-clients .section__heading {
		margin-bottom: 0;
	}

	.our-clients .embla__button {
		top: 62%;
	}

	.address__select_wrapper, .address__select {
		width: 100%;
	}

	.type__header {
		padding: 0 20px;
		height: 150px;
	}

	.type__heading {
		font-size: 1.4rem;
		display: inline-block;
		width: 70%;
	}

	.type__header img {
		width: 100px;
	}

	.documents__heading {
		margin-bottom: 20px;
	}

	.documents__link {
		margin-bottom: 15px;
	}

	.documents__block:not(:last-child) {
		margin-bottom: 15px;
	}

	.contacts__item {
		flex-direction: column;
	}

	.contacts__links {
		width: 100%;

	}
	.contacts__map {
		width: 100%;
		margin-top: 20px;
	}





}


@media screen and (max-width: 482px) {
	.services__item:not(.services__item_large) {
		width: 100%;
		height: auto;
		min-height: 240px;
	}

	.services__item_large {
		min-height: 260px;
	}

	.services__item:nth-child(1) {
		margin-bottom: 20px;
	}

	.about-us__image {
		width: 80%;
		margin-top: 0;
	}

	.about-us__heading {
		font-size: 2rem;
		line-height: 1.3;
	}
}

@media screen and (max-width: 410px) {
	.advantages__item {
		width: 100%;
		min-height: auto;
	}

	.callback {
		padding: 15px;
	}

	.footer__nav {
		width: 100%;
		height: auto;

		&:nth-of-type(2) {
			//margin-top: 0;
		}
	}

	.footer__li:last-child {
		margin-bottom: 10px;
	}

	.footer__copyright {
		margin-top: 10px;
		margin-bottom: 10px;
	}

	.type__header {
		position: relative;
		height: 120px;
	}

	.type__header img {
		width: 80px;
		position: absolute;
		bottom: 5px;
		right: 5px;
	}

	.breadcrumbs__item:not(:last-of-type) {
		margin-right: 20px;

		&::after {
			right: -14px;
		}


	}

	.breadcrumbs__item span, .breadcrumbs__link {
		font-size: 0.65rem;
	}

	.railway__heading, .multimodal__heading {
		font-size: 1.2rem;
	}



}